<?xml version="1.0"?>
<launch>

  <node pkg="gopigo_controller" type="gopigo_controller.py" name="gopigo_controller" output="screen">
    <param name="rate" value="5" />
    <param name="motor_max_angular_vel" value="4.0" />
    <param name="motor_min_angular_vel" value="1.22" />
    <param name="motor_cmd_max" value="100" />
    <param name="motor_cmd_min" value="30" />
    <param name="robot_wheel_radius" value="0.04" />
    <param name="gopigo_on" value="True" />
  </node>


</launch>


